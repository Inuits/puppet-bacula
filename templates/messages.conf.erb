
Messages {
  name                    = "<%= title %>"
<% if !mailcommand.empty? -%>
  mailcommand             = "<%= mailcommand %>"
<% end -%>
<% if !operatorcommand.empty? -%>
  operatorcommand         = "<%= operatorcommand %>"
<% end -%>
<% _messages.each do | type, dest, message | -%>
  <%= type %>             = "<%= dest %>" = "<%= message %>"
<% end -%>
}
