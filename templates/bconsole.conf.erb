#
# Managed by puppet
#
# Bacula Console Configuration File
#

#
# List Directors we connect to
#

Director {
  Name = <%= scope.lookupvar('bacula::config::director_label') %>
  DIRport = 9101
  address = <%= scope.lookupvar('bacula::config::director_server') %>.<%= domain %>
  Password = "<%= scope.lookupvar('bacula::config::director_password') %>"
}
