
Job {
	Name = "<%= name %>"
  	Client = <%= hostname %>-fd
	JobDefs = "<%= jobdefs %>"
	Write Bootstrap = "/var/spool/bacula/Client1.bsr"
	Storage = <%= hostname %>-storage
}

Job {
	Name = "<%= name %>-res"
	Type = Restore
	Client = <%= hostname %>-fd
	FileSet = "FullSet"
	Storage = <%= hostname %>-storage
	Pool = Default
	Messages = Standard
	Where = /tmp/bacula-restores
}
