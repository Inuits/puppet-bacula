Job {
	Name = "<%= hostname %>"
  	Client = <%= hostname %>-fd
	JobDefs = "DefaultJob"
	Write Bootstrap = "/var/spool/bacula/Client1.bsr"
}

Job {
	Name = "<%= hostname %>-res"
	Type = Restore
	Client = <%= hostname %>-fd
	FileSet = "FullSet"
	Storage = File
	Pool = Default
	Messages = Standard
	Where = /tmp/bacula-restores
}
